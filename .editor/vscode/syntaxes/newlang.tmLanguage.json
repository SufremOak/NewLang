{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "NewLang",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.double-slash.newlang",
					"match": "//.*$"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.newlang",
					"match": "\\b(req|func|exit|inv|as|Stage|io\\.println\\*ascii|fs\\.scanFilesystem|fs\\.createVariable|for|do|on|Main)\\b"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.newlang",
					"begin": "\"",
					"end": "\"",
					"patterns": [
						{
							"name": "constant.character.escape.newlang",
							"match": "\\\\."
						}
					]
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.newlang",
					"match": "\\b\\d+\\b"
				}
			]
		}
	},
	"scopeName": "source.newlang"
}